<div id="masthead" style="padding-top: 8px; padding-bottom: 8px;">
	<div class="container">
		<div class="masthead-pad">
			<div class="masthead-text">
				<div style="float: left; width: 300px;">
					<img border="0" src="/img/ringmail_logo1.png"/>
				</div>
				<div style="float: left; margin-left: 30px; width: 260px;">
					<h2>Internet Switchboard</h2>
					<p style="padding-left: 10px;">Receive calls to your Email Address</p>
					<p style="padding-left: 10px;">Call Email Addresses<!-- &amp; Domain Names--></p>
				</div>
				<div style="float: left; margin-left: 30px; width: 260px;">
					<form method="post" action="[% page.url('path' => '/login') %]" style="margin: 0px; padding: 0px;">
						<table style="width: 200px;">
							<tbody>
								<tr>
									<td>
										<div><strong>RingMail Login:</strong></div>
										<div class="control-group" style="margin-bottom: 0px;">
											<div class="controls" style="margin-bottom: 0px;">
												<input type="text" id="login" name="login" value="" placeholder="Your Email Address" class="login username-field" style="width: 140px; margin-bottom: 4px;"/>
											</div>
										</div>
										<div class="control-group" style="margin-bottom: 0px;">
											<div class="controls" style="margin-bottom: 0px;">
												<input type="password" id="password" name="password" value="" placeholder="Password" class="login password-field" style="width: 140px; margin-bottom: 4px;"/>
											</div>
										</div>
									</td>
									<td style="padding-left: 10px;">
										<button class="button btn btn-info"><i class="icon-user"></i> Sign&nbsp;In</button>
									</td>
								</tr>
							</tbody>
						</table>
					</form>
				</div>
				<br clear="all"/>
			</div> <!-- /.masthead-text -->
		</div>
	</div> <!-- /.container -->	
</div> <!-- /#masthead -->
<div id="content">
	<div class="container">
		<div class="row">
			<div class="span7">
				<table class="table">
					<thead>
						<tr>
							<th>RingMail Calling Gateways:</th>
						</tr>						
					</thead>
					<tbody>
					<tr>
						<td>
							<span class="label label-success">Web Gateway</span>
							<br/>
							<br/>
							<form method="post" action="[% page.url() %]" style="margin: 0px; padding: 0px;">
								<table class="table table-bordered table-striped table-highlight">
									<thead>
										<tr>
											<th colspan="2">&nbsp;&nbsp;Call RingMail Address:</th>
										</tr>
									</thead>
									<tbody>
										<tr>
											<td width="65%" style="padding-bottom: 0px;">
												<div class="control-group" style="margin-bottom: 0px;">
													<label for="did_1" class="control-label">Email Address<!-- or Domain Name-->:</label>
													<div class="controls" style="margin-bottom: 0px;">
														[% page.field(
'command' => 'webgw',
'type' => 'text',
'name' => 'target',
'opts' => {'class' => 'input-large', 'id' => 'target', 'placeholder' => 'Email To Call'},

														) %]
													</div>
												</div>
												<div class="control-group" style="margin-bottom: 0px;">
													<label for="did_1" class="control-label">Your Phone Number:</label>
													<div class="controls" style="margin-bottom: 0px;">
														[% page.field(
'command' => 'webgw',
'type' => 'text',
'name' => 'caller_did',
'opts' => {'class' => 'input-large', 'id' => 'caller_did', 'placeholder' => 'Number Calling From'},

														) %]
													</div>
													<p>RingMail calls this number to connect.</p>
												</div>
											</td>
											<td width="35%">
												<p>Enter the email address<!-- or domain--> of the person you would like to call.</p>
												<div>
													[% page.button(
'command' => 'webgw',
'contents' => '<i class="icon-bell"></i> Ring',
'opts' => {'class' => 'btn btn-success btn-large'},
													) %]
												</div>
											</td>
										</tr>
									</tbody>
								</table>
							</form>
						</td>
					</tr>
					<tr>
						<td>
							<span class="label label-secondary">SMS Gateway</span>
							<br/>
							<br/>
							<h1 style="display: inline;">RINGML&nbsp;&nbsp;</h1>
							<h2 style="display: inline;">746465</h2>
							<p>
								Text <em>email@address.com</em> <!--or <em>domain.com</em> -->to <strong>746465</strong> on your mobile phone.
								<br/>
								<br/>
								After RingMail receives your text, your phone will ring.<br/>
								Answer this call and press "1" to connect to the party you're calling.
							</p>
						</td>
					</tr>
					<!--<tr>
						<td><span class="label label-success">App Gateway</span></td>
						<td class="who">Posted by Bill</td>
					</tr>
					<tr>
						<td><span class="label label-danger">SIP Gateway</span></td>
						<td class="who">Posted by Bill</td>
					</tr>-->
					</tbody>
				</table>	
			</div> <!-- /.span8 -->
			<div class="span5">
				<div style="font-size: 1.7em; margin-bottom: 8px;">New to RingMail?</div>
				<div style="font-size: 1.7em; margin-bottom: 8px;">Sign Up for Free:</div>
				<div style="margin-bottom: 16px;">
					<a class="btn btn-success" href="/signup">Register New RingMail Account &raquo;</a>
				</div>
				<hr/>
				<!--<p>Register your account to begin receiving calls from:</p>-->
				<table class="table stat-table">
					<tbody>
						<tr>
							<td class="value">Email</td>
							<td class="full">Register "<em>your@email.com</em>"</td>
						</tr>
						<!--<tr>
							<td class="value">Domain</td>
							<td class="full">Register "<em>yourdomain.com</em>"</td>
						</tr>-->
						<tr>
							<td class="value">Free</td>
							<td class="full">Free calls using VoIP</td>
						</tr>
						<tr>
							<td class="value">Connect</td>
							<td class="full">Receive calls on any phone<!--<br/>Requires subscription--><!-- <a href="/pro">Subscribe Now &raquo;</a>--></td>
						</tr>
						<tr>
							<td class="value">Mobile</td>
							<td class="full">RingMail App for iPhone<br/>Receive RingMaill calls for free</td>
						</tr>
						<tr>
							<td class="value">Desktop</td>
							<td class="full">Receive calls to any SIP phone:<br/>Desktop phones and softphones</td>
						</tr>
						<!--<tr>
							<td class="value">Ring</td>
							<td class="full">Join the party with the <a href="#">Ring Protocol</a></td>
						</tr>-->
					</tbody>
				</table>
			</div> <!-- /.span4 -->
		</div> <!-- /.row -->
	</div> <!-- /.container -->
</div> <!-- /#content -->
